{
  "name": "but-ui",
  "version": "0.1.22",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
	"dev": "vue-cli-service serve",
    "docs": "vue-cli-service build --mode build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name but-ui --dest lib packages/index.js"
  },
  "dependencies": {
    "@vusion/md-vue-loader": "^1.2.4",
    "less": "2.7.3",
    "less-loader": "^5.0.0",
    "vue": "^2.6.10",
    "vue-hljs": "^1.1.2",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "description": "这是一个基于 Vue 2.0 的桌面端组件库",
  "main": "babel.config.js",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Re-dial/ButUI.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Re-dial/ButUI/issues"
  },
  "homepage": "https://github.com/Re-dial/ButUI#readme"
}
